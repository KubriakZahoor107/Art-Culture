{
  "name": "server",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "module",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/src/tests/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.[tj]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [],
    "testEnvironment": "node",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "moduleFileExtensions": [
      "js",
      "cjs",
      "ts",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts --exec \"TS_NODE_TRANSPILE_ONLY=1 node --loader ts-node/esm src/index.ts\"",
    "prisma": "prisma",
    "lint": "eslint \"src/**/*.{ts,js}\"",
    "format": "prettier --write .",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.2",
    "nodemailer": "^6.9.16",
    "sharp": "^0.33.5",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.24.0",
    "@eslint/js": "^9.30.1",
    "@eslint/json": "^0.12.0",
    "@prisma/client": "^6.9.0",
    "@types/axios": "^0.9.36",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.7",
    "@types/nodemailer": "^6",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.30.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.2.5",
    "prisma": "^6.11.1",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5",
    "typescript-eslint": "^8.36.0"
  }
}
